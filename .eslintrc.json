{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": 2,
        "valid-jsdoc": 2,
        "max-len": 0,
        "function-paren-newline": "warn",
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "space-in-parens": [
            "error",
            "never"
        ]
    }
}